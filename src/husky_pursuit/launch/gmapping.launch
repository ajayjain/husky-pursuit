	<node pkg="tf" type="static_transform_publisher" name="odom_footprint_link" args="0 0 0 0 0 0 odom_combined
 base_footprint 100" />
	<!--remap from="encoder" to="odom" /-->
	<node pkg="topic_tools" type="relay" name="encoder_relay" args="/encoder /odom_combined" />
	<node pkg="gmapping" type="slam_gmapping" name="slam_gmapping">
